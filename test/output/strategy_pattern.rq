PREFIX woc: <http://rdf.webofcode.org/woc/>

SELECT ?ContextInterface ?AlgorithmInterface ?Context ?Strategy ?ConcreteStrategyA ?ConcreteStrategyB ?ConcreteStrategyC 
 WHERE {
 ?ContextInterface  a woc:Method.
?AlgorithmInterface  a woc:Method.
?Context  a woc:Class.
?Strategy  a woc:Class.
?ConcreteStrategyA  a woc:Class.
?ConcreteStrategyB  a woc:Class.
?ConcreteStrategyC  a woc:Class.
?Context woc:hasMethod ?ContextInterface.
?Strategy woc:hasMethod ?AlgorithmInterface.
?ConcreteStrategyA woc:hasMethod ?AlgorithmInterface.
?ConcreteStrategyB woc:hasMethod ?AlgorithmInterface.
?ConcreteStrategyC woc:hasMethod ?AlgorithmInterface.
?ConcreteStrategyA woc:extends ?Strategy.
?ConcreteStrategyB woc:extends ?Strategy.
?ConcreteStrategyC woc:extends ?Strategy.
?Strategy woc:references ?Context.

}

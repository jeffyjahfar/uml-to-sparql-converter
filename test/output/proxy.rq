PREFIX woc: <http://rdf.webofcode.org/woc/>

SELECT ?Request1 ?RealRequest ?ProxyRequest ?Subject ?RealSubject ?Proxy 
 WHERE {
 ?Request1  a woc:Method .
?Request1 woc:hasModifier woc:Public .
?Request1 woc:hasModifier woc:Abstract .
?RealRequest  a woc:Method .
?RealRequest woc:hasModifier woc:Public .
?ProxyRequest  a woc:Method .
?ProxyRequest woc:hasModifier woc:Public .
?Subject  a woc:Class .
?Subject woc:hasModifier woc:Public .
?Subject woc:hasModifier woc:Abstract .
?RealSubject  a woc:Class .
?RealSubject woc:hasModifier woc:Public .
?Proxy  a woc:Class .
?Proxy woc:hasModifier woc:Public .
?Subject woc:hasMethod ?Request1 .
?RealSubject woc:hasMethod ?RealRequest .
?Proxy woc:hasMethod ?ProxyRequest .
?RealSubject woc:extends ?Subject .
?Proxy woc:extends ?Subject .
?Proxy woc:references ?RealSubject .

}

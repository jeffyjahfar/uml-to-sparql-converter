PREFIX woc: <http://rdf.webofcode.org/woc/>

SELECT ?FactoryMethod ?AnOperation ?Product ?ConcreteProduct ?Creator ?ConcreteCreator 
 WHERE {
 ?FactoryMethod  a woc:Method.
?AnOperation  a woc:Method.
?Product  a woc:Class.
?ConcreteProduct  a woc:Class.
?Creator  a woc:Class.
?ConcreteCreator  a woc:Class.
?Creator woc:hasMethod ?FactoryMethod.
?Creator woc:hasMethod ?AnOperation.
?ConcreteCreator woc:hasMethod ?FactoryMethod.
?ConcreteProduct woc:extends ?Product.
?ConcreteProduct woc:hasDependency ?ConcreteCreator.
?ConcreteCreator woc:extends ?Creator.

}

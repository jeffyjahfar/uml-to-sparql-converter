PREFIX woc: <http://rdf.webofcode.org/woc/>

SELECT ?Target, Client2 ?Adaptee5 ?Adapter10 ?Request1 ?SpecificRequest4 ?Request8 
 WHERE {
 ?Target, Client2  a woc:Class .
?Target, Client2 woc:hasModifier woc:Abstract .
?Adaptee5  a woc:Class .
?Adapter10  a woc:Class .
?Request1  a woc:Method .
?SpecificRequest4  a woc:Method .
?Request8  a woc:Method .
?Adapter10 woc:extends ?Target, Client2 .
?Target, Client2 woc:hasMethod ?Request1 .
?Adaptee5 woc:hasMethod ?SpecificRequest4 .
?Adapter10 woc:hasMethod ?Request8 .

}

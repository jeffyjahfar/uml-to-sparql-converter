PREFIX woc: <http://rdf.webofcode.org/woc/>

SELECT ?Request ?Handle ?Context ?State ?ConcreteStateA ?ConcreteStateB 
 WHERE {
 ?Request  a woc:Method.
?Handle  a woc:Method.
?Context  a woc:Class.
?State  a woc:Class.
?ConcreteStateA  a woc:Class.
?ConcreteStateB  a woc:Class.
?Context woc:hasMethod ?Request.
?State woc:hasMethod ?Handle.
?ConcreteStateA woc:hasMethod ?Handle.
?ConcreteStateB woc:hasMethod ?Handle.
?ConcreteStateA woc:extends ?State.
?ConcreteStateB woc:extends ?State.
?Context woc:references ?State.

}

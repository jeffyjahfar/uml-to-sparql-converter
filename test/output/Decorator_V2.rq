PREFIX woc: <http://rdf.webofcode.org/woc/>

SELECT ?Component0 ?ConcreteComponent2 ?Decorator4 ?MediateDecorator5 ?ConcreteDecoratorA7 ?ConcreteDecoratorB9 
 WHERE {
 ?Component0  a woc:Class .
?Component0 woc:hasModifier woc:Abstract .
?ConcreteComponent2  a woc:Class .
?Decorator4  a woc:Class .
?Decorator4 woc:hasModifier woc:Abstract .
?MediateDecorator5  a woc:Class .
?ConcreteDecoratorA7  a woc:Class .
?ConcreteDecoratorB9  a woc:Class .
?Decorator4 woc:extends ?Component0 .
?ConcreteComponent2 woc:extends ?Component0 .
?ConcreteDecoratorA7 woc:extends ?Decorator4 .
?ConcreteDecoratorB9 woc:extends ?Decorator4 .

PREFIX woc: <http://rdf.webofcode.org/woc/>

SELECT ?Operation1 ?Operation2 ?Operation3 ?Class1 ?Class2 ?Class3 ?Class4 ?Attribute1 ?Attribute2 ?Attribute3 
 WHERE {
 ?Operation1  a woc:Method.
?Operation1 woc:hasModifier woc:Public .
?Operation2  a woc:Method.
?Operation2 woc:hasModifier woc:Public .
?Operation3  a woc:Method.
?Operation3 woc:hasModifier woc:Protected .
?Class1  a woc:Class.
?Class1 woc:hasModifier woc:Public .
?Class1 woc:hasModifier woc:Abstract .
?Class2  a woc:Class.
?Class2 woc:hasModifier woc:Public .
?Class3  a woc:Class.
?Class3 woc:hasModifier woc:Public .
?Class4  a woc:Class.
?Class4 woc:hasModifier woc:Public .
?Attribute1  a woc:Field.
?Attribute1 woc:hasModifier woc:Protected .
?Attribute2  a woc:Field.
?Attribute2 woc:hasModifier woc:Public .
?Attribute3  a woc:Field.
?Attribute3 woc:hasModifier woc:Private .
?Class1 woc:hasMethod ?Operation1.
?Operation1 woc:hasReturnType ?Class3.
?Class2 woc:hasMethod ?Operation1.
?Operation1 woc:hasReturnType ?Class4.
?Class2 woc:hasMethod ?Operation2.
?Operation2 woc:hasParameter ?Parameter1.
?Parameter1 woc:hasType ?Class3.
?Class2 woc:hasMethod ?Operation3.
?Class2 woc:extends ?Class1.
?Class2 woc:hasDependency ?Class2.
?Class2 woc:hasField ?Attribute2.
?Class2 woc:hasField ?Attribute3.
?Class2 woc:hasField ?Attribute1.
?Class4 woc:extends ?Class3.
?Class2 woc:hasDependency ?Class4.

}

PREFIX woc: <http://rdf.webofcode.org/woc/>

SELECT ?Request1 ?Request ?SpecificRequest ?Target ?Adapter ?Adaptee ?adaptee 
 WHERE {
 ?Request1  a woc:Method .
?Request1 woc:hasModifier woc:Public .
?Request1 woc:hasModifier woc:Abstract .
?Request  a woc:Method .
?Request woc:hasModifier woc:Public .
?SpecificRequest  a woc:Method .
?SpecificRequest woc:hasModifier woc:Public .
?Target  a woc:Class .
?Target woc:hasModifier woc:Public .
?Target woc:hasModifier woc:Abstract .
?Adapter  a woc:Class .
?Adapter woc:hasModifier woc:Public .
?Adaptee  a woc:Class .
?Adaptee woc:hasModifier woc:Public .
?adaptee  a woc:Field .
?adaptee woc:hasModifier woc:Public .
?Target woc:hasMethod ?Request1 .
?Adapter woc:hasMethod ?Request .
?Adaptee woc:hasMethod ?SpecificRequest .
?Adapter woc:extends ?Target .
?Adapter woc:references ?Adaptee .

}

<?xml version="1.0"?>
<xmitransformations version="2.0" requirexmiid="false">

<xmitransformation modelelement="sdmetricsbase" xmipattern="base" condition="1=0">
<trigger name="id" type="attrval" attr="xmi.id" />
</xmitransformation>

<xmitransformation modelelement="class" xmipattern="UML:Class" 
 	condition="arg1='hi' &amp; arg2=2" requirexmiid="true">
<trigger name="visibility" type="attrval" attr="visibility"/>
</xmitransformation>

<xmitransformation modelelement="operation" xmipattern="UML:Class" 
 	condition="(arg1='hi' | arg2='') &amp; arg3!=''">
</xmitransformation>

<xmitransformation modelelement="task" xmipattern="UML:Class" 
	condition="!(arg1='ho' | arg2=2)">
</xmitransformation>

<xmitransformation modelelement="component" xmipattern="UML:Class" >
</xmitransformation>

</xmitransformations>

<?xml version="1.0"?>
<xmitransformations version="2.0">
<!-- XMI transformation with a condition expression that does not yield a boolean -->
<xmitransformation modelelement="sdmetricsbase" xmipattern="base" condition="a+b">
<trigger name="id" type="attrval" attr="xmi.id" />
</xmitransformation>
<xmitransformation modelelement="class" xmipattern="UML:Class" 
 	condition="'hi'">
<trigger name="visibility" type="attrval" attr="visibility"/>
</xmitransformation>
</xmitransformations>

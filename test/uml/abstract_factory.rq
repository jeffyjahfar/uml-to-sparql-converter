PREFIX woc: <http://rdf.webofcode.org/woc/>

SELECT ?createProductA ?createProductB ?AbstractFactory ?ConcreteFactory1 ?ConcreteFactory2 ?AbstractProductA ?ProductA2 ?ProductA1 ?AbstractProductB ?ProductB2 ?ProductB1 
 WHERE {
 ?createProductA  a woc:Method.
?createProductB  a woc:Method.
?AbstractFactory  a woc:Class.
?ConcreteFactory1  a woc:Class.
?ConcreteFactory2  a woc:Class.
?AbstractProductA  a woc:Class.
?ProductA2  a woc:Class.
?ProductA1  a woc:Class.
?AbstractProductB  a woc:Class.
?ProductB2  a woc:Class.
?ProductB1  a woc:Class.
?AbstractFactory woc:hasMethod ?createProductA.
?AbstractFactory woc:hasMethod ?createProductB.
?ConcreteFactory1 woc:hasMethod ?createProductA.
?ConcreteFactory1 woc:hasMethod ?createProductB.
?ConcreteFactory2 woc:hasMethod ?createProductA.
?ConcreteFactory2 woc:hasMethod ?createProductB.
?ConcreteFactory1 woc:extends ?AbstractFactory.
?ConcreteFactory2 woc:extends ?AbstractFactory.
?ProductA2 woc:extends ?AbstractProductA.
?ProductA1 woc:extends ?AbstractProductA.
?ProductA2 woc:hasDependency ?ConcreteFactory2.
?ProductA1 woc:hasDependency ?ConcreteFactory1.
?ProductB2 woc:extends ?AbstractProductB.
?ProductB1 woc:extends ?AbstractProductB.
?ProductB2 woc:hasDependency ?ConcreteFactory2.
?ProductB1 woc:hasDependency ?ConcreteFactory1.

}
